generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Meter {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  model       String
  tsCreated   DateTime @db.Date
  tsModified  DateTime @db.Date
}
