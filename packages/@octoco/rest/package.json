    "@nestjs/core": "^10.1.2",
    "@nestjs/platform-express": "^10.1.2",
    "@nestjs/swagger": "^7.1.6",
    "@octoco/models": "workspace:^",
    "@types/express": "^4.17.17",
    "class-validator": "^0.14.0",
    "firebase-admin": "^11.10.1",
    "mongodb-memory-server": "^8.13.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
