    "esbuild": "^0.18.18",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
