  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.app.json",
    "build:prod": "node esbuild.js",
    "build:docker": "docker build -f ./Dockerfile -t octoco-rest ..",
